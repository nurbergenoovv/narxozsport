{
  "expo": {
    "name": "SportApp",
    "slug": "sportapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NarxozSport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#E10303"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nurbergenovv.narxozsport"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NarxozSport.png",
        "backgroundColor": "#E10303"
      },
      "package": "com.nurbergenovv.narxozsport",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow to access your photos"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e04d135b-fb5c-484c-bfa0-26208de6aa8d"
      }
    },
    "owner": "nurbergenovv"
  }
}
