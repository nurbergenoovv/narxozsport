{
  "expo": {
    "name": "SportApp",
    "slug": "SportApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NarxozSport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#CC0836"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nurbergenovv.narxozsport"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NarxozSport.png",
        "backgroundColor": "#CC0836"
      },
      "package": "com.nurbergenovv.narxozsport"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission":"Allow to access your photos"	
        }
      ]
    ]
  }
}
